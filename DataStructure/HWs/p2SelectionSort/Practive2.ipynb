def selectionSorting(arr):
    for i in range(len(arr)-1, 0, -1):
        max_i = 0
        max_v = 0
        for j in range(i):
            if arr[j] > max_v:
                max_v = arr[j]
                max_i = j
        if arr[i] < max_v:
            temp = arr[i]
            arr[i], arr[max_i] = arr[max_i], temp
    return arr
